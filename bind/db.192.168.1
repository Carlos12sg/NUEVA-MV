;
; BIND reverse data file for local loopback interface
;
$TTL	604800
1.168.192.in-addr.arpa.		IN	SOA	server.private.izv. root.private.izv. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
1.168.192.in-addr.arpa.		IN	NS	server.private.izv.
150.1.168.192.in-addr.arpa.	IN	PTR	server.private.izv.
